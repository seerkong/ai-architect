{
  "name": "ai-architect",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "node scripts/build-all.cjs",
    "merge-assets": "node scripts/merge-assets.cjs",
    "start": "node scripts/start-production.cjs",
    "dev": "concurrently \"npm run shared-dev\" \"npm run browser-dev\" \"npm run server-dev\"",
    "shared-build": "node scripts/build-shared.cjs",
    "shared-dev": "node scripts/watch-shared.cjs",
    "browser-dev": "node scripts/dev-browser.cjs",
    "browser-preview": "cd browser && vite preview",
    "server-dev": "npm run merge-assets && cd server && npm run dev"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  },
  "stackblitz": {
    "startCommand": "pnpm i && pnpm dev"
  }
}