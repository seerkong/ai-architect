{
  "name": "ai-architect-server",
  "version": "1.0.0",
  "description": "Koa server for ai",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r src/aiagent/prompt dist/aiagent/",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/koa-route": "^3.2.8",
    "@types/markdown-it": "^14.1.2",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^8.3.4",
    "config": "^4.1.1",
    "fast-xml-parser": "^5.2.5",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-route": "^4.0.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^7.0.0",
    "markdown-it": "^14.1.0",
    "mysql2": "^3.14.4",
    "openai": "^4.20.1",
    "razor-js-template": "^1.0.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.37.7",
    "toml": "^3.0.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.1",
    "@types/jest": "^30.0.0",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.9",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^20.8.0",
    "jest": "^30.1.1",
    "less": "^4.4.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  }
